<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solovyevs.memonotes">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Settings"></activity>
        <activity android:name=".AccessActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewNote" />
        <activity android:name=".PincodeEditor" />
        <activity android:name=".KeyboardFragment" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>